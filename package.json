    "style-loader": "^3.3.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3"
